<%= form_for(@song) do |f| %>
 
    <%= f.label :title %>
    <%= f.text_field :title %>


    <%= f.label :release_date %>
    <%= f.text_field :release_date %>

    <%= f.label :album %>
    <%= f.text_field :album %>

    <%= f.label :artist_id %>
    <%= f.collection_select :artist_id, Artist.all, :id, :name, prompt: true %>

 

    <%= f.submit "Save song" %>

   
    <% end %>
